{
    "name": "ascetic-soft/wirebox",
    "description": "Lightweight, zero-config PSR-11 dependency injection container for PHP 8.4+ with autowiring, PHP attributes, compiled container generation, and built-in dotenv support",
    "keywords": [
        "dependency-injection",
        "di",
        "container",
        "autowiring",
        "psr-11",
        "attributes",
        "dotenv",
        "service-container",
        "ioc"
    ],
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "AsceticSoft\\Wirebox\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AsceticSoft\\Wirebox\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Andrey Borodulin",
            "email": "borodulin@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.4",
        "psr/container": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "phpstan/phpstan": "^2.1",
        "friendsofphp/php-cs-fixer": "^3.94"
    }
}
